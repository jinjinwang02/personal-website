{
  "name": "personal-website",
  "version": "1.0.0",
  "description": "Personal website 2020",
  "main": "app.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss public/css/style.css -w",
    "start-app": "node app.js",
    "start": "npm-run-all -p watch:sass start-app",
    "compile:sass": "node-sass sass/main.scss public/css/style.comp.css",
    "concat:css": "concat -o public/css/style.concat.css public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/style.comp.css -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "dependencies": {
    "autoprefixer": "^9.8.0",
    "body-parser": "^1.19.0",
    "concat": "^1.0.3",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "live-server": "^1.2.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1"
  },
  "devDependencies": {},
  "author": "Jinjin Wang",
  "license": "ISC"
}